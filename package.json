  },
  "devDependencies": {
    "expect.js": "0.3.1",
    "fastbench": "1.0.1",
    "mocha": "2.4.5",
    "mocha-circleci-reporter": "0.0.1",
    "neo-async": "1.7.4",
    "uglify-js": "2.6.2"
